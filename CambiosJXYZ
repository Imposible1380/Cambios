    <b:includable id='schema-jsonld'>
      <script type="application/ld+json">
        {
          "@context":"http://schema.org",
          "@type": "BlogPosting",
          "image": "<b:eval expr='data:post.firstImageUrl ? data:post.firstImageUrl : data:imgDefault'/>",
          "url": "<data:post.url.canonical/>",
          "headline": "<data:post.title/>",
          "dateCreated": "<data:post.timestampISO8601/>",
          "datePublished": "<data:post.timestampISO8601/>",
          "dateModified": "<data:post.lastUpdatedISO8601/>",
          "inLanguage": "<data:blog.locale/>",
          "isFamilyFriendly": "<b:eval expr='data:blog.adultContent ? "false" : "true"'/>",
          "author": {
            "@type": "Person",
            "name": "<data:post.author/>",
            "url": "<data:post.authorProfileUrl/>"
          },
          "publisher": {
            "@type": "Organization",
            "name": "<data:blog.title/>",
            "url": "<data:blog.homepageUrl.canonical/>",
            "logo": {
              "@type": "ImageObject",
              "url": "logo-amp.png",
              "width":"233",
              "height":"60"
            }
          },
          "mainEntityOfPage": "true",<b:if cond='data:view.isPost and data:post.labels'>
          "keywords": [<b:loop index='li' values='data:post.labels' var='i'>"<data:i.name/>"<b:if cond='data:post.labels.length != (data:li + 1)'>,</b:if></b:loop>],
          "genre":[<b:loop index='li' values='data:post.labels' var='i'>"<data:i.name/>"<b:if cond='data:post.labels.length != (data:li + 1)'>,</b:if></b:loop>],</b:if>
          "articleBody": "<b:eval expr='data:post.body.jsonEscaped'/>"
        }
      </script>
    </b:includable>
    <b:includable id='shareButtons'>
      <b:if cond='data:post.sharePostUrl'>
        <div class="display-flex flex-wrap align-items-center">
          <b:class expr:name='data:view.isMultipleItems ? "card__space card__share" : "post__share bottom-space"'/>
          <a class='btn-border wjs-window' expr:href='params(data:post.sharePostUrl,{target: "facebook"})'><i class='fab fa-facebook-f fa-mn fa-fw'/></a>
