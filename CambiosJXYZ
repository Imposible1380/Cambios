          <a class='btn-border wjs-window' expr:href='params(data:post.sharePostUrl,{target: "googlePlus"})'><i class='fab fa-google-plus-g fa-mn fa-fw'/></a>
          <a class='btn-border wjs-window' expr:href='params(data:post.sharePostUrl,{target: "pinterest"})'><i class='fab fa-pinterest-p fa-mn fa-fw'/></a>
          <b:if cond='data:view.isSingleItem'>
            <a class='btn-border wjs-window' expr:href='params("//www.tumblr.com/widgets/share/tool/preview",{canonicalUrl: data:post.url.canonical, caption: data:post.snippet, posttype: "link", title: data:post.title})'><i class='fab fa-tumblr fa-mn fa-fw'/></a>
            <b:if cond='data:top.showEmailButton'>
              <a class='btn-border wjs-window' expr:href='params(data:post.sharePostUrl,{target: "email"})'><i class='fas fa-at fa-mn fa-fw'/></a>
