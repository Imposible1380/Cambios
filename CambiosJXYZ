/* Animations */

/*jello*/
@keyframes jello {
  from,
  10%,
  to {
    transform: translate3d(0, 0, 0);
  }
  25% {
    transform: skewX(-6.5deg) skewY(-6.5deg);
  }
  50% {
    transform: skewX(3.5deg) skewY(3.5deg);
  }
  90% {
    transform: skewX(-0.5deg) skewY(-0.5deg);
  }
}

.ta-jello li:hover > ul,
.ta-jello .gallery__href:hover span::after,
.ta-jello .card__image:hover::before {
    animation-name: jello;
}

/*wobble*/
@keyframes wobble {
  from {
    transform: translate3d(0, 0, 0);
  }
  25% {
    transform: translate3d(-10%, 0, 0) rotate3d(0, 0, 1, -3deg);
  }

  50% {
    transform: translate3d(5%, 0, 0) rotate3d(0, 0, 1, 2deg);
  }

  75% {
    transform: translate3d(-2%, 0, 0) rotate3d(0, 0, 1, -1deg);
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}

.ta-wobble li:hover > ul,
.ta-wobble .gallery__href:hover span::after,
.ta-wobble .card__image:hover::before {
    animation-name: wobble;
}

/*tada*/
@keyframes tada {
  from {
    -webkit-transform: scale3d(1, 1, 1);
    transform: scale3d(1, 1, 1);
  }

  20% {
    transform: scale3d(0.9, 0.9, 0.9) rotate3d(0, 0, 1, -3deg);
  }

  30%, 50% {
    transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg);
  }

  40%, 60% {
    transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg);
  }

  to {
    transform: scale3d(1, 1, 1);
  }
}

.ta-tada li:hover > ul,
.ta-tada .gallery__href:hover span::after,
.ta-tada .card__image:hover::before {
    animation-name: tada;
}

/*swing*/
@keyframes swing {
  20% {
    transform: rotate3d(0, 0, 1, 15deg);
  }

  40% {
    transform: rotate3d(0, 0, 1, -10deg);
  }

  60% {
    transform: rotate3d(0, 0, 1, 5deg);
  }

  80% {
    transform: rotate3d(0, 0, 1, -5deg);
  }

  to {
    transform: rotate3d(0, 0, 1, 0deg);
  }
}

.ta-swing li:hover > ul,
.ta-swing .gallery__href:hover span::after,
.ta-swing .card__image:hover::before {
    animation-name: swing;
}

